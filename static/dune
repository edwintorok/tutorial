(rule
  (targets inputs.tex)
  (deps    %{bin:otb-build}
           ../book/toc.scm)
  (action  (run otb-build build inputs -o . -repo-root ..)))

(rule
  (targets book.aux book.idx book.toc)
  (deps    (alias ../book/latex)
           (:x ../book/book.tex)
           ../book/dedication.tex
           ../book/author_bio.tex
           ../book/appendix.tex
           inputs.tex)
  (action  (system "pdflatex -interaction=nonstopmode %{x} -draftmode")))

(rule
  (targets book.ind)
  (deps    (:x book.idx))
  (action  (system "makeindex %{x}")))

(alias (name pdf) (deps book.pdf))

(rule
  (targets book.pdf)
  (deps    (alias ../book/latex)
           (:x ../book/book.tex)
           ../book/dedication.tex
           ../book/author_bio.tex
           ../book/appendix.tex
           inputs.tex
           book.aux
           book.ind
           book.toc)
  (action  (system "pdflatex -interaction=nonstopmode %{x}")))
